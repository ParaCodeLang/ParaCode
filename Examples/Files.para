// Creating
CREATEFILE("text.txt")

// Reading
VAR fileBefore = READFILE("text.txt")
PRINT(fileBefore)

// Writing
WRITEFILE("text.txt", "test")
VAR fileAfterOne = READFILE("text.txt")
PRINT(fileAfterOne)

// Appending
APPENDFILE("text.txt", "test")
VAR fileAfterTwo = READFILE("text.txt")
PRINT(fileAfterTwo)

// Deleting
DELETEFILE("text.txt")